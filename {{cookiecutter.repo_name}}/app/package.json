{
  "name": "{{ cookiecutter.repo_name }}",
  "private": true,
  "prettier": "@thorgate/prettier-config",
  "scripts": {
    "dev": "razzle start",
    "predev": "yarn clean",
    "clean": "rimraf build",
    "build": "razzle build",
    "prebuild": "yarn clean",
    "test": "razzle test --env=jsdom",
    "start": "NODE_ENV=production node build/server.js",
    "export-assets": "cp -uR ./build/public/static /files/assets/",
    "extract-i18n": "i18next-scanner --config ./scanner.config.js",
    "prettier-check": "prettier --check",
    "prettier-check-all": "yarn prettier-check 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier-format": "prettier --write",
    "prettier-format-all": "yarn prettier-format 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "eslint ./src/",
    "stylelint": "stylelint 'src/**/*.scss'"
  },
  "dependencies": {
    "@loadable/babel-plugin": "5.5.0",
    "@loadable/component": "5.5.0",
    "@loadable/server": "5.5.0",
    "@loadable/webpack-plugin": "5.5.0",
    "@sentry/node": "5.20.1",
    "@sentry/react": "5.20.1",
    "@tg-resources/fetch": "3.1.3",
    "@tg-resources/fetch-runtime": "3.1.1",
    "@tg-resources/redux-saga-router": "3.1.3",
    "@thorgate/babel-preset": "^1.0.0",
    "@thorgate/spa-components": "1.0.0-alpha.39",
    "@thorgate/spa-entities": "1.0.0-alpha.40",
    "@thorgate/spa-errors": "1.0.0-alpha.39",
    "@thorgate/spa-forms": "1.0.0-alpha.40",
    "@thorgate/spa-hooks": "1.0.0-alpha.39",
    "@thorgate/spa-is": "1.0.0-alpha.39",
    "@thorgate/spa-pending-data": "1.0.0-alpha.39",
    "@thorgate/spa-permissions": "1.0.0-alpha.39",
    "@thorgate/spa-view": "1.0.0-alpha.39",
    "@thorgate/spa-view-manager": "1.0.0-alpha.39",
    "babel-core": "^7.0.0-bridge.0",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "connected-react-router": "6.4.0",
    "date-fns": "2.5.0",
    "formik": "1.5.8",
    "history": "4.9.0",
    "i18next": "19.8.9",
    "i18next-fetch-backend": "3.0.0",
    "i18next-fs-backend": "1.0.8",
    "i18next-http-middleware": "3.1.0",
    "i18next-multiload-backend-adapter": "1.0.0",
    "is-docker": "2.0.0",
    "is_js": "0.9.0",
    "js-cookie": "2.2.1",
    "koa": "2.10.0",
    "koa-bodyparser": "4.2.1",
    "koa-cookie": "1.0.0",
    "koa-helmet": "5.2.0",
    "koa-logger": "3.2.1",
    "koa-proxies": "0.9.0",
    "koa-response-time": "2.1.0",
    "koa-router": "7.4.0",
    "koa-static": "4.0.3",
    "koa-useragent": "1.2.1",
    "lodash.template": "4.5.0",
    "lorem-ipsum": "1.0.6",
    "normalizr": "3.4.1",
    "prop-types": "15.7.2",
    "qs": "6.5.2",
    "razzle": "3.4.5",
    "razzle-plugin-eslint": "3.4.5",
    "razzle-plugin-modify-eslint-loader-config": "^1.0.0",
    "razzle-plugin-long-term-caching": "^1.0.0",
    "razzle-plugin-scss": "3.4.5",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet-async": "1.0.7",
    "react-i18next": "11.8.7",
    "react-redux": "7.2.2",
    "react-router": "5.1.2",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "reactstrap": "7.1.0",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "rimraf": "2.6.2",
    "serialize-javascript": "1.5.0",
    "tg-named-routes": "1.0.0-alpha.39",
    "tg-resources": "3.1.3",
    "tg-saga-manager": "1.0.0-alpha.39",
    "throng": "4.0.0",
    "typesafe-actions": "4.4.2",
    "winston": "3.2.1",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@thorgate/eslint-config": "1.0.0",
    "@thorgate/prettier-config": "1.0.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "i18next-scanner": "3.0.0",
    "identity-obj-proxy": "3.0.0",
    "prettier": "2.2.1",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "cobertura"
    ],
    "moduleNameMapper": {
      "\\.module.(css|scss)$": "identity-obj-proxy",
      "@winston": "<rootDir>/node_modules/winston"
    },
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/razzle/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^.+\\.scss$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|scss|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    }
  }
}
