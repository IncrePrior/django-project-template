{
  "name": "{{ cookiecutter.repo_name }}",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "8.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.21",
    "bootstrap": "4.3.1",
    "css-loader": "1.0.0",
    "extract-css-chunks-webpack-plugin": "3.1.1",
    "file-loader": "1.1.11",
    "node-sass": "4.13.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "2.1.5",
    "prop-types": "15.7.2",
    "raven-js": "3.26.3",
    "react": "16.8.5",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "16.8.5",
    "react-redux": "6.0.1",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "resolve-url-loader": "2.3.0",
    "sass-loader": "7.1.0",
    "url-loader": "1.0.1",
    "webpack": "4.17.2",
    "webpack-bundle-tracker": "0.4.0-beta",
    "webpack-cli": "3.1.0"
  },
  "devDependencies": {
{%- if cookiecutter.include_storybook == 'yes' %}
    "@storybook/addon-storyshots": "^5.3.12",
    "@storybook/react": "^5.3.12",
{%- endif %}
    "babel-eslint": "8.2.3",
    "babel-jest": "^23.6.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.7.0",
    "jest": "^23.6.0",
    "jest-dom": "^1.12.0",
    "prettier": "1.18.2",
{%- if cookiecutter.include_storybook == 'yes' %}
    "react-test-renderer": "16.8.5",
{%- endif %}
    "react-testing-library": "^5.0.1",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
        "app/src/**/*.{js,jsx}",
        "!app/src/test-setup.js",
        "!**/node_modules/**"
    ],
    "globals": {
      "NODE_ENV": "test",
      "DJ_CONST": {}
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
{%- if cookiecutter.include_storybook == 'yes' %}
    "moduleNameMapper": {
      "styles-src": "<rootDir>app/src/stylesStub.js",
      "\\.(css|less|scss)$": "<rootDir>app/src/stylesStub.js"
    },
{%- endif %}
    "modulePaths": [
      "<rootDir>/app/src",
      "<rootDir>/node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/app/src/test-setup.js",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
        "<rootDir>/node_modules/"
    ],
    "verbose": true
  },
  "scripts": {
    "dev": "cd app && NODE_ENV=development webpack --config webpack/config.dev.js --watch --progress --profile --color",
    "build": "cd app && NODE_ENV=production webpack -p --config webpack/config.production.js --progress --profile --color",
    "build-quiet": "cd app && NODE_ENV=production webpack -p --config webpack/config.production.js",
    {%- if cookiecutter.include_storybook == 'yes' %}
    "build-storybook": "build-storybook -c app/src/.storybook",
    {%- endif %}

    "clean": "cd app && rm -rf build/",
    "prebuild": "yarn clean",
    "prettier-check": "prettier --check",
    "prettier-check-all": "yarn prettier-check 'app/src/**/*.{js,jsx,ts,tsx}'",
    "prettier-format": "prettier --write",
    "prettier-format-all": "yarn prettier-format 'app/src/**/*.{js,jsx,ts,tsx}'",
    "lint": "cd app/src && eslint .",
    {%- if cookiecutter.include_storybook == 'yes' %}
    "storybook": "start-storybook -c app/src/.storybook -p 80",
    {%- endif %}
    "stylelint": "cd static/styles-src && stylelint '**/*.scss'",
    "test": "NODE_ENV=test jest",
{%- if cookiecutter.include_storybook == 'yes' %}
    "test:update": "NODE_ENV=test jest -u",
{%- endif %}
    "test:watch": "NODE_ENV=test jest --watch"
  }
}
